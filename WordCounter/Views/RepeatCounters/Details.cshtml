@{
    Layout = "_Layout";
    ViewData["Title"] = "Details";
}

<h1>search #@Model.GetId()</h1>
<div class="details">
    <h3>word: @Model.GetTargetWord()</h3>
    <h3>search phrase: @Model.GetSearchPhrase()</h3> 
    <h3>total matches: @Model.GetMatches()</h3>
</div>

<div class="nav-buttons">
    <a href="/repeatcounters"><button class="btn btn-default">home</button></a>
    <a href="/repeatcounters/history"><button class="btn btn-success">history</button></a>
    <a href="/repeatcounters/new"><button class="btn btn-warning">search</button></a>
</div>